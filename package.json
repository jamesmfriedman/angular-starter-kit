{
  "name": "angular-starter-kit",
  "version": "1.0.0",
  "description": "Angular Starter Kit",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --port 8080 --env.BUILD_ENV dev --env.NODE_ENV dev",
    "dev": "npm start",
    "dev:min": "webpack-dev-server --inline --hot --port 8080 --env.BUILD_ENV 'dev optimized' --env.NODE_ENV dev",
    "build:dev": "webpack --env.BUILD_ENV optimized --env.NODE_ENV dev",
    "build:prod": "webpack --env.BUILD_ENV optimized --env.NODE_ENV prod",
    "test": "export NODE_ENV='dev' BUILD_ENV='test' && karma start",
    "postinstall": "typings install",
    "typings": "typings"
  },
  "author": "James Friedman",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "angular-in-memory-web-api": "^0.3.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.12",
    "angular2-template-loader": "^0.5.0",
    "autoprefixer": "^6.4.1",
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.28.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.2",
    "normalize.css": "^6.0.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "typescript": "^2.2.2",
    "typings": "^1.3.3",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  }
}
